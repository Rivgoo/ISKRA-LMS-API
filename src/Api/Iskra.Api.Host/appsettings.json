{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "CustomFormater": {
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss",
      "UseUtcTimestamp": true
    }
  },
  "AllowedHosts": "*",
  "SystemModules": [
    "Iskra.Modules.Validation"
  ],
  "PluginSettings": {
    "RootPath": "./Modules",
    "EnableCleaner": true,
    "EnabledModules": [
      "Iskra.Infrastructure.MariaDb"
    ]
  }
}
